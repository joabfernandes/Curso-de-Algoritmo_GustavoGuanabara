algoritmo "005"
var
   A, B, C: Logico
   C1, C2, C3, C4, C5, C6: Logico
inicio
   A <- Falso
   B <- Verdadeiro
   C <- Falso
   Se A entao
      A <- C1
   SeNao
      Se B entao
         Se C entao
            C <- C2
         SeNao
            B <- C3
            C <- C4
         FimSe
      FimSe
      B <- C5
   FimSe
   B <- C6
fimalgoritmo
